{% comment %} <img src='{{ 'logo.png' |  asset_url}}'/> {% endcomment %}
<h2>{{ collections['frontpage'].title }}</h2>
{% for product in collections['frontpage'].products %}
    <div>
        <a href="{{ product.url }}">
            {{ product.featured_image | image_url: width: 400 |  image_tag }}
            <h3>{{- product.title }}</h3>
            <p>{{ product.price |  money }}</p>
        </a>
    </div>
{% endfor %}
